{
    "name": "propaganistas/laravel-disposable-email",
    "description": "Disposable email validator",
    "keywords": [
        "laravel",
        "mail",
        "email",
        "disposable",
        "throwaway",
        "temporary",
        "validator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "illuminate/cache": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/config": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/validation": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3|^1.4.2",
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4": {
            "Propaganistas\\LaravelDisposableEmail\\": "src/",
            "Propaganistas\\LaravelDisposableEmail\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Propaganistas\\LaravelDisposableEmail\\DisposableEmailServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
